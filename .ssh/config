Include ~/.orbstack/ssh/config

Host *
ControlMaster auto
ControlPath ~/.ssh/sockets/ssh-%r@%h:%p
ControlPersist 60
ServerAliveInterval=120

#returnado account
Host bitbucket.org-returnado
    HostName bitbucket.org
    User git
    IdentityFile ~/.ssh/returnado
    IdentitiesOnly yes

#gfg account
Host bitbucket.org-gfg
    HostName bitbucket.org
    User git
    IdentityFile ~/.ssh/id_rsa
    IdentitiesOnly yes

#gfg account prgs
Host bitbucket.org-gfgp
    HostName bitbucket.org
    User git
    IdentityFile ~/.ssh/gameshub
    IdentitiesOnly yes

Host dev.studygy.com
    Hostname dev.studygy.com
    User andriis
    IdentityFile ~/.ssh/studygy
    IdentitiesOnly yes


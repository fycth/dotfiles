Host *
ControlMaster auto
ControlPath ~/.ssh/sockets/ssh-%r@%h:%p
ControlPersist 60
ServerAliveInterval=15

#returnado account
Host bitbucket.org-returnado
    HostName bitbucket.org
    User git
    IdentityFile ~/.ssh/returnado
    IdentitiesOnly yes

#gfg account
Host bitbucket.org-gfg
    HostName bitbucket.org
    User git
    IdentityFile ~/.ssh/id_rsa
    IdentitiesOnly yes
